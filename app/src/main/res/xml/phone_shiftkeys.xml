<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="20%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >

    <Row android:horizontalGap="0%p" android:keyHeight="0.00%p">
        <Key android:codes="32" android:keyLabel=""
            android:keyWidth="100%p"
            />
    </Row>

    <Row>
        <Key android:codes="40" android:keyLabel="(" android:keyEdgeFlags="left" android:horizontalGap="10%p" />
        <Key android:codes="47" android:keyLabel="/" />
        <Key android:codes="41" android:keyLabel=")" />
        <Key android:codes="45" android:keyLabel="-" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:codes="78" android:keyLabel="N" android:keyEdgeFlags="left" android:horizontalGap="10%p" />
        <Key android:codes="44" android:keyLabel="Pause"/>
        <Key android:codes="44" android:keyLabel=","/>
        <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:codes="42" android:keyLabel="*" android:keyEdgeFlags="left" android:horizontalGap="10%p" />
        <Key android:codes="59" android:keyLabel=";"/>
        <Key android:codes="35" android:keyLabel="\#"/>
        <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" android:keyEdgeFlags="right" android:isRepeatable="true"/>
    </Row>
    
    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="-11" android:keyLabel="123" android:keyEdgeFlags="left" android:horizontalGap="10%p" />
        <Key android:codes="43" android:keyLabel="+" />
        <Key android:codes="46" android:keyLabel="." android:isRepeatable="true"/>
        <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>
    